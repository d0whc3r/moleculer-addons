    "ts-node": "10.9.2",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">= 8.x.x"
  },
  "jestSonar": {
    "reportPath": "../../coverage",
    "reportFile": "slack-test-report.xml"
  },
  "jest-junit": {
    "outputDirectory": "../../coverage",
