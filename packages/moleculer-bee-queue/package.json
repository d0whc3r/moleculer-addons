    "test:old": "jest -i",
    "test:coverage": "yarn test --coverage"
  },
  "peerDependencies": {
    "moleculer": "^0.14.16"
  },
  "dependencies": {
    "bee-queue": "^1.4.0",
    "redis": "^3.1.2"
